<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zpop.web.dao.MeetingDao">
    <insert id="insert" parameterType="com.zpop.web.entity.Meeting">
        insert into meeting
        (regMemberId, categoryId, regionId, ageRangeId, title, content, detailRegion, maxMember, startedAt, contact)
        values(#{regMemberId}, #{categoryId}, #{regionId}, #{ageRangeId}, #{title}, #{content}, #{detailRegion}, #{maxMember}, #{startedAt}, #{contact})
    </insert>

    <select id="get" resultType= "com.zpop.web.entity.Meeting">
        select *
        from meeting
        where id=#{id}
    </select>

    <select id="getThumbList" resultType= "com.zpop.web.view.MeetingThumbView">
        select *
        from meetingThumb
    </select>

<!--    <update id="update" parameterType="com.zpop.web.entity.Meeting">-->
<!--        update meeting-->
<!--        SET readAt = '1999-07-19';-->
<!--    </update>-->
</mapper>

